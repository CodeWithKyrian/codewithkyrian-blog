{
    "name": "codewithkyrian/codewithkyrian-blog",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "calebporzio/sushi": "^2.5",
        "codeat3/blade-simple-icons": "^5.7",
        "codewithkyrian/whisper.php": "1.1.0",
        "filament/filament": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "graham-campbell/markdown": "^15.2",
        "laravel/framework": "^11.9",
        "laravel/socialite": "^5.16",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "shuvroroy/filament-spatie-laravel-backup": "^2.2",
        "spatie/laravel-medialibrary": "^11.9",
        "spatie/laravel-sitemap": "^7.2",
        "tool5/torchlight-commonmark": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.1",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "codewithkyrian/platform-package-installer": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/codewithkyrian/torchlight-commonmark-php.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
